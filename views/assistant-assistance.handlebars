<div class="auxiliar-assistance mb-3">
  <div class="auxiliar-assistance__header">
    <div class="auxiliar-assistance__test">

    </div>
  </div>
  <div class="auxiliar-assistance__search">
    <form action="">
      <input type="text" placeholder="Buscar Estudiante...">
    </form>
  </div>
  <div class="auxiliar-assistance__assistance">
    <table class="table table-hover auxiliar-assistance__table">
      <thead>
        <tr class="text-white">
          <th>Estudiante</th>
          <th>Edad</th>
          <th>Grado</th>
          <th>Seccion</th>
          <th>Presente</th>
          <th>Ausente</th>
          <th>Restraso</th>
        </tr>
      </thead>
      <tbody>
        {{#each students}}
        <tr>
          <td>{{dni}}</td>
          <td>{{name}}</td>
          <td>{{last_name}}</td>
          <td>{{'Assistance.TotalAssistence'}}</td>
          <td>{{'Assistance.Total'}}</td>
          <td>{{'Assistance.Total'}} </td>
          <td></td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>